{
    "manifest_version": 3,
    "name": "Lunar Loader",
    "description": "Client for IO Games and websites! Made by Alreadycode!",
    "version": "1.5",
    "action": {
        "default_popup": "popup/keycheck.html"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "scripting",
        "activeTab",
        "storage"
    ]
}
